<script src="scripts/bootstrap-formhelpers-phone.en_US.js"></script>
<script src="scripts/bootstrap-formhelpers-phone.js"></script>
<script src="/scripts/ion.rangeSlider.js"></script>
<script src="scripts/main.js"></script>


<link href="http://fonts.googleapis.com/css?family=Josefin+Slab" rel="stylesheet" type="text/css">
<link href="http://fonts.googleapis.com/css?family=Josefin+Sans" rel="stylesheet" type="text/css">

<link rel="stylesheet" href="stylesheets/style.css">

<h1 class="text-center"> People </h1>

<body>
  <link rel='stylesheet' href='stylesheets/style.css'>
  <div class="container">
  {{#grouped_each 6 people}}
      {{#each this}} 
      <div class="row match">
        <div class="col-md-8 col-md-offset-6">
          <h2 class="text-center">{{name}}</h2>
          <p class="matchcontact text-center">{{email}}</p>
          <p class="matchcontact text-center">{{phone}}</p>
          <div id="pars-{{@index}}" class="collapse text-center">
            <p class="matchinfo">{{bio}}</p>
            <p class="matchinfo">{{tech_background}}</p>
            <p class="matchinfo">{{goals}}</p>
          </div>
          <p class="btn revealer text-center" data-num="{{@index}}">Details</p>
        </div>
      </div>
     {{/each}}
  {{/grouped_each}}
</div>
<body/>
</html>